{
  "version" : 1,
  "lineLength" : 120,
  "indentation" : {
    "spaces" : 4
  },
  "tabWidth" : 4,
  "respectsExistingLineBreaks" : true,
  "maximumBlankLines" : 1,
  "rules" : {
    "DoNotUseSemicolons" : true,
    "UseShorthandTypeNames" : true,
    "NeverForceUnwrap" : false,
    "NeverUseImplicitlyUnwrappedOptionals" : true
  }
}
